varnishtest "Test a german IP address"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	import geoip from "${vmod_topbuild}/src/.libs/libvmod_geoip2.so";

	sub vcl_init {
		geoip.init("/var/lib/mmdb/GeoLite2-City.mmdb");
	}

	sub vcl_deliver {
		set resp.http.X-Country-Code = geoip.country_code("85.10.240.255");
		set resp.http.X-Country-Name = geoip.country_name("85.10.240.255");
		set resp.http.X-Region-Name = geoip.region_name("85.10.240.255");
		set resp.http.X-City-Name = geoip.city_name("85.10.240.255");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.X-Country-Code == DE
	expect resp.http.X-Country-Name == Germany
	expect resp.http.X-Region-Name == Bavaria
	expect resp.http.X-City-Name == Munich
} -run
